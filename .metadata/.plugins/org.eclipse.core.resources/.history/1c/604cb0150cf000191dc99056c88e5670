package animations;

public class LeftArmAnimator extends Animator {
    protected Shape shape;
    
    public LeftArmAnimator(DrawingPanel drawingPanel) {
    	super(drawingPanel);
    }
    
    public void setShape(Shape shape) {
    	this.shape = shape;
    }

	public void draw() {
		shape.changeLeftArm();
	}

}
